  "query": "\n        SELECT o.* FROM organizations o\n        INNER JOIN organization_memberships om ON o.id = om.organization_id\n        WHERE om.user_id = $1 AND om.is_active = true\n        ORDER BY o.created_at DESC\n        LIMIT $2 OFFSET $3\n        ",
